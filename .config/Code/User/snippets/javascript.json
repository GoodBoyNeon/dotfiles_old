	"command module for discord.js": {
		"prefix": "cmd",
		"body": [
		  "const { MessageEmbed, MessageActionRow, MessageButton } = require(\"discord.js\");",
		  "",
		  "module.exports = {",
		  "    name: '$1',",
		  "    description: '$2'",
		  "    run: async (client, interaction) => {",
		  "        ",
		  "    },",
		  "};"
		],
		"description": "command module for discord.js"
	},
	"try block for djs": {
		"prefix": "err",
		"body": [
			"try {",
			"    $1",
			"} catch(err) {",
			"    console.error(err)",
			"    interaction.reply({",
			"        content: 'Error executing the command, please ring the devs!',",
			"        ephemeral: true,",
			"    });",
			"}",
		],
		"description": "try block for interaction in discord.js"
	}
}